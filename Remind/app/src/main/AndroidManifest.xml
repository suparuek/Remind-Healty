<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stripz.remind">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="Remind Healthy"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FromRegisterActivity" />
        <activity android:name=".CalculateActivity" />
        <activity android:name=".BMIover" />
        <activity android:name=".BMIunder" />
        <activity android:name=".BMInormal" />
        <activity android:name=".PlanActivity" />
        <activity android:name=".UpdateActivity"></activity>
    </application>

</manifest>